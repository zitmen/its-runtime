struct Result
{
	int cost;
	int weight;
}

void main()
{
	Result result;
	for(int i = 0; i < 10; i++)
	{
		result = computeSolution(i);
		print("c=");
		print(int2str(result.cost));
		print(",w=");
		println(int2str(result.weight));
	}
}

Result computeSolution(int i)
{
	Result retval = new;
	retval.cost = i*i;
	retval.weight = i*i*i;
	return retval;
}
