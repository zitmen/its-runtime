void main()
{
	for(int i = 1; i < 10; i++)
		println(fact(i));
}

String fact(int x)
{
	int f = 1;
	for(int i = 2; i <= x; i++)
		f *= i;
	return to(f);
}

String to(int x)
{
	String f = int2str(x);
	String s = concat(concat(f, ",len="), int2str(strlen(f)));
	if(endsWith(f, "0") || endsWith(f, "1"))
		return concat(s, ", true");
	//else
		return concat(s, ", false");
}
